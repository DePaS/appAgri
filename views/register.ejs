<head>
    <script src="http://code.jquery.com/jquery-3.1.0.min.js"></script>
    <link rel='stylesheet' href='/app.css' />
</head>

<body> 
    <div id="register">
        <img src="https://image.freepik.com/free-vector/artistic-leaf-logo-design-agriculture-company_100735-16.jpg">
        <h2>REGISTER</h2>
        <form action="/register" method="POST" id="invia" autocomplete="off">
                <input autocomplete="new-password" type="text" id="name" name="name" placeholder="User" >
                <input type="email" id="email" name="email" placeholder="E-Mail" >
                <input type="password" id="password" name="password" placeholder="Password">
            <button type="submit">Register</button>
        </form>
        <a href="/login">Login</a>
        <a href="/home">Home</a>
    </div>
</body>